#include <stdio.h>
#include <stdlib.h>

int main()
{
    for(int i=33; i<=126; i++){
        printf("ASCII code for %c is %d\n",i,i);
    }
    return 0;
}

#include <stdio.h>
#include <stdlib.h>
int forNumber(int smallLetter);
int main()
{
    while(1){
    char smallLetter, capitalLetter;
    printf("please enter a small letter: ");
    smallLetter=getchar();
    if(1==forNumber(smallLetter)){
        printf("the number is %c", smallLetter);
    }
    else{
        capitalLetter=smallLetter-32;
        printf("the capital letter is: %c\n", capitalLetter);
    }
    printf("\n\n");
    }
    return 0;


}

int forNumber(int smallLetter)
{
    if(smallLetter>='0' && smallLetter<='9'){
        return 1;
    }
    else{
    return 0;
    }
    return 0;
}

#include<stdio.h>
#include<stdlib.h>
int forNumber(int smallLetter);
int main()
{
    while(1){
    char smallLetter, capitalLetter;
    printf("please enter a small letter: ");
    smallLetter=getchar();
    if(1==forNumber(smallLetter)){
        printf("the number is %c\n", smallLetter);
    }
    else{
        capitalLetter=smallLetter-32;
        printf("the capital letter is: %c\n", capitalLetter);
        }
    }
    return 0;
}

int forNumber(int smallLetter)
{
    if(smallLetter>='0' && smallLetter<='9'){
        return 1;
    }
    else{
        return 0;
    }
    return 0;
}

#include<stdio.h>
int main()
{
    char bot='A', bot2='B';
    int n=100, n2=1000;

    printf("value of bot: %c\t\t\t", bot);
    printf("adress of bot: %p\n", &bot);

    printf("value of bot2: %c\t\t\t", bot2);
    printf("adress of bot2: %p\n", &bot2);

    printf("value of n: %c\t\t\t", n);
    printf("adress of n: %p\n", &n);

    printf("value of n2: %c\t\t\t", n2);
    printf("adress of n2: %p\n", &n2);

    return 0;

}

#include <stdio.h>
#include <stdlib.h>

int main()
{
    int ara[5]={50,60,70,80,90};
    printf("value of Array: %d %d %d %d %d\n", ara[0],ara[1],ara[2],ara[3],ara[4]);
    printf("Adress of ara is %p\n", ara);
    printf("Adress of ara[0] is %p\n", &ara[0]);
    printf("Adress of ara[1] is %p\n", &ara[1]);
    printf("Adress of ara[2] is %p\n", &ara[2]);
    printf("Adress of ara[3] is %p\n", &ara[3]);
    printf("Adress of ara[4] is %p\n", &ara[4]);
    return 0;
}
#include <stdio.h>
#include <stdlib.h>

int main()
{
    printf("size of doubel %d byte \n", sizeof(double));

    return 0;
}


